[alias]
  aa = add --all
  br = branch
  co = checkout
  cb = checkout -b
  cm = commit -m
  cl = clean -i
  df = diff --color-words --ignore-space-at-eol -b -w --ignore-blank-lines
  lg = log --graph --oneline
  st = status
  ps = push
  pl = pull
[core]
  editor = vim
[user]
  name = a_name
  email = an_email@gmail.com
[credential]
  helper = store
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
